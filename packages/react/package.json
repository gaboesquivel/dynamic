{
  "name": "@vencura/react",
  "version": "0.0.1",
  "private": true,
  "description": "React hooks for Vencura API using TanStack Query",
  "license": "PROPRIETARY",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./provider": {
      "types": "./dist/types/provider.d.ts",
      "require": "./dist/cjs/provider.cjs",
      "import": "./dist/esm/provider.mjs"
    },
    "./hooks/use-wallets": {
      "types": "./dist/types/hooks/use-wallets.d.ts",
      "require": "./dist/cjs/hooks/use-wallets.cjs",
      "import": "./dist/esm/hooks/use-wallets.mjs"
    },
    "./hooks/use-tokens": {
      "types": "./dist/types/hooks/use-tokens.d.ts",
      "require": "./dist/cjs/hooks/use-tokens.cjs",
      "import": "./dist/esm/hooks/use-tokens.mjs"
    },
    "./hooks/keys": {
      "types": "./dist/types/hooks/keys.d.ts",
      "require": "./dist/cjs/hooks/keys.cjs",
      "import": "./dist/esm/hooks/keys.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/react-query": "^5.90.8",
    "@vencura/core": "workspace:*",
    "@vencura/lib": "workspace:*",
    "@vencura/types": "workspace:*",
    "nanoid": "^5.0.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "3.25.76",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.32.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.0.0"
  }
}
